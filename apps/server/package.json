{
  "name": "@douglasneuroinformatics/server",
  "version": "0.0.1",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "nest build",
    "check-types": "tsc --noEmit",
    "cli": "NODE_ENV=development nest start --entryFile cli -- $@",
    "debug": "nest start --debug --watch",
    "demo": "NODE_ENV=demo node ./dist/cli.js init-demo && NODE_ENV=demo yarn start",
    "dev": "NODE_ENV=development nest start --watch",
    "format": "prettier --write .",
    "lint": "eslint --fix src",
    "prebuild": "rm -rf dist",
    "start": "node dist/main",
    "test": "yarn test:e2e && yarn test:unit",
    "test:cov": "jest --coverage",
    "test:e2e": "jest -i --config ./test/config/jest-e2e.config.cjs",
    "test:unit": "jest"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/mongoose": "^7.1.3",
    "@douglasneuroinformatics/common": "*",
    "@douglasneuroinformatics/instruments": "*",
    "@douglasneuroinformatics/utils": "latest",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.0.12",
    "ajv": "^8.12.0",
    "bcrypt": "^5.1.0",
    "mongoose": "^7.3.1",
    "nest-commander": "^3.11.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "simple-statistics": "^7.8.3",
    "unidecode": "^0.1.8"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/unidecode": "^0.1.1",
    "jest": "29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "tsconfig-paths": "4.2.0"
  }
}
