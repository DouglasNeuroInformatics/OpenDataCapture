    "@import-meta-env/cli": "^0.7.2",
    "@import-meta-env/unplugin": "^0.6.2",
    "@opendatacapture/demo": "workspace:*",
    "@opendatacapture/instrument-interpreter": "workspace:*",
    "@opendatacapture/instrument-renderer": "workspace:*",
