{
  "name": "@douglasneuroinformatics/ddcp",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "license": "AGPL-3.0",
  "scripts": {
    "build": "env-cmd cross-env NODE_ENV=production turbo run build",
    "check-types": "turbo run check-types",
    "clean": "fd -H -I --prune \".turbo|dist|node_modules\" --exec rm -rf",
    "demo": "env-cmd turbo run demo",
    "dev": "env-cmd cross-env NODE_ENV=development turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish",
    "storybook": "yarn ws client storybook",
    "test": "env-cmd turbo run test",
    "ws": "sh -c 'yarn workspace \"@douglasneuroinformatics/$0\" \"${@:1}\"'"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@douglasneuroinformatics/eslint-config": "^0.0.4",
    "@douglasneuroinformatics/tsconfig": "*",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=9.0.0 <10.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@douglasneuroinformatics/utils": "0.2.1"
  }
}
